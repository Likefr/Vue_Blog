<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rz.mapper.UserMapper">

    <select id="getUserByUsername" resultType="com.rz.entity.User">
        SELECT *
        FROM t_user
        WHERE username = #{username};
    </select>


    <select id="queryAllUser" resultType="com.rz.entity.User">
        SELECT user_id, nickname, email, enable_flag, create_date FROM t_user
    </select>
</mapper>
